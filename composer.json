{
    "name": "alu/advent-of-code-in-php8",
    "description": "The complete collection of my solutions to Advent Of Code in PHP 8 - https://adventofcode.com",
    "minimum-stability": "stable",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Oskar Andersson",
            "email": "alu@byteberry.net"
        }
    ],
    "require": {
        "php": ">=8.1",
        "symfony/console": "~5.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.5.5",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Alu\\": "src/Alu"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alu\\": "src/Alu",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}