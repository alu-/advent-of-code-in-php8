{
    "name": "alu/advent-of-code-in-php8",
    "description": "The complete collection of my solutions to Advent Of Code in PHP 8 - https://adventofcode.com",
    "minimum-stability": "stable",
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Oskar Andersson",
            "email": "alu@byteberry.net"
        }
    ],
    "require": {
        "php": ">=8.5",
        "symfony/console": "~8.0"
    },
    "require-dev": {
        "ext-dom": "*",
        "ext-libxml": "*",
        "phpunit/phpunit": "^12.0.0",
        "squizlabs/php_codesniffer": "3.*",
        "symfony/dotenv": "~8.0",
        "guzzlehttp/guzzle": "~7.10.0",
        "symfony/finder": "~8.0",
        "symfony/filesystem": "~8.0",
        "league/html-to-markdown": "5.0.2",
        "twig/twig": "^3.0",
        "phpstan/phpstan": "^2.1",
        "symfony/var-dumper": "*",
        "brianium/paratest": "^v7.16.1"
    },
    "autoload": {
        "psr-4": {
            "Alu\\": "src/Alu"
        },
        "files": [
            "src/Alu/AdventOfCode/Helpers/ArrayFunctions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Alu\\": "src/Alu",
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/paratest",
        "phpcs": "vendor/bin/phpcs . --colors --cache --standard=./phpcs.xml -p -n"
    }
}
